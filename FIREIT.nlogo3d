;;consumer procedure
to-report choose-provider [ a ] ;; returns chosen provider
  let trust-values []
  ;let has-trust [] ;; list of providers who already have a trust value
  ;let no-trust [] ;; list of providers who don't have a trust value yet
  let value "null"
  let er 0
  let chosen 0
  ask a [
    let has-trust-tmp has-trust
    let no-trust-tmp no-trust
    ask my-providers [
      set value (compute-it myself self)
      ;show value
      ifelse value = "null"
      [
        if not member? self no-trust-tmp
        [set no-trust-tmp (turtle-set self no-trust-tmp)]
      ]
      [
        if not member? self has-trust-tmp
        [
          ask no-trust-tmp [set no-trust-tmp no-trust-tmp with [self != myself]]
          set has-trust-tmp (turtle-set self has-trust-tmp)
        ]
      ]
      set trust-values lput value trust-values

    ]
    set has-trust has-trust-tmp
    set no-trust no-trust-tmp
  ]
  ;show no-trust
  ;show has-trust
  ;show trust-values
  ifelse count no-trust = 0
  [
    set chosen max-one-of my-providers [performance-level] ;; if there are no providers without a trust value, choose the best one
    ;show "NO-TRUST IS EMPTY, CHOOSING BEST PROVIDER"
    ;show count no-trust
    report chosen
  ]
  [
    ;show no-trust
    if not any? has-trust
    [
      set chosen one-of no-trust ;; if it's the consumer's first interaction, choose a random provider
      ;show "HAS-TRUST IS EMPTY, CHOOSING RANDOM PROVIDER"
      report chosen
    ]

    set er expected-return a trust-values
    ;show er
    if er = "null" [report one-of no-trust] ;; should not happen because of previous test
    ifelse er <= random-float 1.0
    [
      report max-one-of my-providers [performance-level]
      ;show "CHOSE ACTION A1 (BEST PROVIDER : EXPLOIT)"
    ]
    [
      report one-of no-trust
      ;show "CHOSE ACTION A2 (RANDOM PROVIDER IN NO-TRUST : EXPLORE)"
    ]
  ];; compute expected return of choosing a provider without a trust value and choose it based on formula
   ;[report one-of my-providers]
end

to-report expected-return [ a trust-values ]
  let er-1 0
  let er-2 0
  let trust-values-providers remove "null" trust-values
  ;show trust-values-providers
  ask a [
    ifelse any? has-trust
    [set er-1 -1 ]
    [set er-1 exp ((max [performance-level] of my-providers ) / temperature )]
    set er-2 er-1 + exp ( mean [performance-level] of has-trust / temperature )
  ]
  ifelse er-1 = -1
  [report "null"]
  [report (er-1 / er-2)]
end

to add-rating [a b c v]
  ask a  [
    ifelse length filter [rating -> item 1 rating = b] ratings >= H ;; if b has more than H ratings
    [
      set ratings remove item 0 (filter [rating -> item 1 rating = b] ratings) ratings ;; remove first rating for b
      set ratings lput (list a b count-interactions c v) ratings
    ]
    [set ratings lput (list a b count-interactions c v) ratings]
  ]
end

