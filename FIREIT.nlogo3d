breed [ consumers consumer ]
breed [ providers provider ]

globals [ count-interactions overall-ug temperature lambda]
consumers-own [ ratings r_o my-providers my-providers-dist has-trust no-trust ug  activity-level]
providers-own [ r_o performance-level performance-level-label my-consumers]

to setup
  clear-all
  set temperature initial-temperature ;; temperature for exploit vs. exploration dilemma
  set count-interactions 0 ;; number of round of interactions
  set overall-ug 0
  set lambda (-(5 / ln(0.5) )) ;; recency factor used for computing interaction trust
  set-default-shape turtles "circle"
  setup-consumers
  setup-providers
  reset-ticks
end

to go
  ask consumers [ ;; choose a provider then give him a rating
    if activity-level <= random-float 1.0
    [
      if any? my-providers [
        let chosen (choose-provider self)
        ;show chosen
        ;set count-interactions count-interactions + 1
        ;set ug (get-performance-level chosen) * min list 1 (radius-operations-provider / (item (position chosen my-providers) my-providers-dist)) ;; quality is degraded linearly in proportion to the distance between c and p
        set ug [performance-level] of chosen * min list 1 (radius-operations-provider / oiseau-dist self chosen) ;; quality is degraded linearly in proportion to the distance between c and p
        ;show (get-performance-level chosen)
        ;show list 1 ((item (position chosen my-providers) my-providers-dist) / radius-operations-provider)
        ;show list 1 (radius-operations-provider / (item (position chosen my-providers) my-providers-dist))
        ;show list 1 (1 - radius-operations-provider / (item (position chosen my-providers) my-providers-dist))
        ;show ug
        ;show (item (position chosen my-providers) my-providers-dist) / radius-operations-provider
        add-rating self chosen "quality" ug / 10
;        ifelse mean-ug-providers = "null"
;        [set mean-ug-providers ug] ;; first interaction
;        ;[set mean-ug-providers (( mean-ug-providers + ug ) / length my-providers )]
;        [set mean-ug-providers (( mean-ug-providers + ug ) / count has-trust )]
        ;show mean-ug-providers
      ]
    ]
  ]
  let test one-of consumers
  ;show [no-trust] of test
  ;show [has-trust] of test
  ;show count-interactions
  ;if count-interactions mod 100 = 0 [ set temperature temperature - 1 ]
  set temperature ceiling ( initial-temperature - count-interactions  / 2)
  if temperature <= 1 [ set temperature 1 ]
  if count-interactions > 0
  [set overall-ug mean [ug] of consumers with [count has-trust != 0]]
  ;show [ug] of consumers with [any? has-trust]
  ;show [ug] of consumers with [length has-trust != 0]
  ;set overall-ug compute-overall-ug
  set count-interactions count-interactions + 1
  tick
end

to setup-consumers
  create-consumers initial-number-consumers ;
  [
    set my-providers turtle-set no-turtles;; list of providers in my radius of operations
    set my-providers-dist [];; list of distances of providers
    set ratings [] ;; list of ratings : R(a,b,c) r=(a,b,i,c,v)
    set has-trust no-turtles ;; list of providers who already have a trust value
    set no-trust no-turtles ;; list of providers who don't have a trust value yet
    ;set omega-it [] ;; current IT rating weight
    set ug "null"
    ;set mean-ug-providers "null" ;; unused
    ;set activity-level random-float 1.0
    set activity-level (random-float 1.0 + min-activity-level) - min-activity-level
    set color red
    set size 1
    tilt-up asin (1.0 - random-float 2.0)
    ;tilt-up (random 1.0) * 2.0 - 1
    jump max-pxcor
    ;set number who
  ]
end

to setup-providers
  let PL_PERFECT 10
  let PL_GOOD 5
  let PL_OK 0
  let PL_BAD -5
  let PL_WORST -10
  create-providers NP ;
  [
    ;set performance-level one-of [-10 -5 0 5 10] ;; performance level
    set performance-level "null"
    set my-consumers no-turtles
    set color red
    tilt-up asin (1.0 - random-float 2.0)
    jump max-pxcor
    ;set number who
  ]
  ask n-of NPG providers with [performance-level = "null"]
  [
;    set performance-level random-normal ((PL_GOOD + PL_PERFECT) / 2) sig-good
    let mu-good random-float (PL_PERFECT - PL_GOOD) + PL_GOOD
    set performance-level random-normal mu-good sig-good
    if performance-level > 10 [ set performance-level 10 ]
    set performance-level-label "good"
    set color green + 2
  ]
  ask n-of NPO providers with [performance-level = "null"]
  [
;    set performance-level random-normal ((PL_OK + PL_GOOD) / 2) sig-ordinary
    let mu-ordinary random-float (PL_GOOD - PL_OK) + PL_OK
    set performance-level random-normal mu-ordinary sig-good
    if performance-level > 10 [ set performance-level 10 ]
    set performance-level-label "ordinary"
    set color green
  ]
  ask n-of NPI providers with [performance-level = "null"]
  [
    set performance-level random-float (PL_GOOD - PL_BAD) + PL_BAD
    if performance-level > 10 [ set performance-level 10 ]
    set performance-level-label "intermittent"
    set color blue
  ]
  ask n-of NPB providers with [performance-level = "null"]
  [
;    set performance-level random-normal ((PL_WORST + PL_OK) / 2) sig-bad
    let mu-bad random-float (PL_OK - PL_WORST) + PL_WORST
    set performance-level random-normal mu-bad sig-good
    if performance-level < -10 [ set performance-level -10 ]
    set performance-level-label "bad"
    set color green - 2
  ]
  setup-provider-lists
end

to setup-provider-lists ;; list of providers in a consumer's range
  ask consumers [
    let cpt 0
    let tmp-providers no-turtles
    ask providers [
      let min_corde (2 - 2 * cos radius-operations-consumer) * max-pxcor
      ;show min_corde
      ;if min_corde < distance myself
      ; [
      ;show distance myself
      set cpt cpt + 1
      if provider-is-in-range myself self [
        ;set tmp-providers lput who tmp-providers
        set tmp-providers (turtle-set tmp-providers self)
        set my-consumers (turtle-set my-consumers myself)
      ]
      ; ]
    ]
    set my-providers tmp-providers
    ;show cpt
  ]
end

