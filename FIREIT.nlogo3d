to-report provider-is-in-range [ a b ] ;; returns true if provider b is in consumer a 's range
  if oiseau-dist a b <= radius-operations-consumer [
    report true
  ]
  report false
end

to-report oiseau-dist [ a b ]
  let b_coord []
  let corde2 0
  let oiseau 0
  let my-range 0
  let r-sphere max-pxcor
  ask b [
    set b_coord lput (xcor / r-sphere) b_coord
    set b_coord lput (ycor / r-sphere) b_coord
    set b_coord lput (zcor / r-sphere) b_coord  ;; triplet of b's coordinates
  ]
  ;show b_coord
  ask a [
    set corde2 corde2 + (item 0 b_coord - (xcor / r-sphere))^ 2
    set corde2 corde2 + (item 1 b_coord - (ycor / r-sphere))^ 2
    set corde2 corde2 + (item 2 b_coord - (zcor / r-sphere))^ 2 ;; (x2 - x1) + (y2 - y1) + (z2 - z1)
                                                                ;show corde2
  ]
  report acos ( 1 - corde2 / 2 )
end

;;a b i c v
;; compute interaction trust component
to-report compute-it [ a b ]
  let it 0
  ;let len-rating 1
  let ratings-with-b []
  ;let omega [] ;; old omega method, now with recency factor lambda ( omega_ri = exp( -(count-interactions - item 2 rating / lambda ))
  ;let sum-omega 0
  ;let rating-cpt []
  ask a [
    ;   foreach ratings [ rating ->
    ;      if item 1 rating = b [
    ;       set ratings-with-b lput item 4 rating ratings-with-b
    ;       ;set len-rating len-rating + 1
    ;      ]
    ;    ]
    set ratings-with-b filter [rating -> item 1 rating = b] ratings ;; faster method
    ;show ratings-with-b
    ;set len-rating length ratings
    ;set rating-cpt n-values len-rating [ i -> i + 1 ]
    ;set sum-omega sum rating-cpt
    ;foreach rating-cpt [ v -> set omega lput (v / sum-omega) omega ]
  ]
  ;show ratings-with-b
  ifelse (empty? ratings-with-b) [ report "null" ]
  [;show ratings-with-b
   ;show empty? ratings-with-b
    let cpt 0
    while [ cpt < length ratings-with-b ] [
      ;show cpt
      ;show len-rating
      ;set it it + ((item cpt ratings-with-b) * (item cpt omega)) ;;old method
      ;set it it + ((item 4 item cpt ratings-with-b) * (item cpt omega)) ;;faster old method
      set it it + ((item 4 item cpt ratings-with-b) * exp (- (count-interactions - (item 2 item cpt ratings-with-b)) / lambda )) ;; faster method with recency factor
      set cpt cpt + 1
    ]
    report it
  ]
  ;show omega
  ;show ratings-with-b
  ;report it
end
